{%- comment -%} https://web.dev/uses-rel-preconnect/ {%- endcomment -%}
{%- capture preftchUrls -%}
  //monorail-edge.shopifysvc.com;
  //cdn.shopify.com;
  //cdnjs.cloudflare.com;
{%- endcapture -%}
{%- liquid
  assign urls = preftchUrls | split: ';'
  for url in urls
    assign u = url | strip
    echo '<link rel="preconnect" href="' | append: u | append: '" />'
  endfor
-%}