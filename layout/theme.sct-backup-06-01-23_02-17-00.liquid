      <main class="o-main" id="MainContent" role="main">
        <div class="o-main__wrapper">
          
          {%- comment -%} Static (Site Wide) Sections {%- endcomment -%}
          {%- section 'header' -%}
          
          {{ content_for_layout }}

          {%- section 'footer' -%}
          {%- section 'promotions' -%}
          {%- section 'cart-drawer' -%}
          {%- section 'free-gwp-message' -%}
        </div>
      </main>
